<body>
  <section class="report-header">
    <%if (schoolname) { %>
    <h3 class="report-author"><%= schoolname %></h3>
    <% } else { %>
    <h3 class="report-author">UE. Cardenal Gonzalez Zumarraga</h3>
    <% } %>
    <%if (schoolicon) { %>
    <img class="icon-school" src="<%= schoolicon %>" />
    <% } else { %>
    <img class="icon-school" src="https://s3-us-east-2.amazonaws.com/gonzu-api-bucket/2020/09/apple-touch-icon-1.png" />
    <% } %>
    <h1 class="report-title">Reporte votaciones <%= toUpperCase(voter_tag) %></h1>
    <h4 class="report-subtitle"><%= name %></h4>
  </section>
  <section style="margin-left:auto;margin-right:auto;width: 100%;" class="report-content-container">
    <table style="margin-left:auto;margin-right:auto;">
      <tr>
        <th>#</th>
        <% voter_keys.forEach(voter_field => { %>
        <th><%= voter_field %></th>
        <% }); %>
        <th>Total</th>
      </tr>
      <% voters[voter_tag].forEach(function(voter,index){%>
        <tr>
          <td><%= index + 1 %></td>
          <% voter_keys.forEach(voter_field => { %>
            <% if (hasVote(votes_group.group_by_tag[voter_tag],voter) != -1) { %>
              <td class="centered-text p-3" ><%= voter[voter_field] %></td>
            <% } else { %>
              <td class="centered-text p-3 card-soft"><%= voter[voter_field] %></td>
            <% } %>
          <% }); %>
          <% if (hasVote(votes_group.group_by_tag[voter_tag],voter) != -1) { %>
            <td class="centered-text p-3"><%= campaigns[votes_group.group_by_tag[voter_tag][hasVote(votes_group.group_by_tag[voter_tag],voter)].campaign].name %></td>
          <% } else { %>
            <td class="centered-text p-3 card-soft">-</td>
          <% } %>
        </tr>
      <% }); %>
    </table>
  </section>
</body>

