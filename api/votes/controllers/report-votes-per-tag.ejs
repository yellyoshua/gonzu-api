      border-radius: 10px;
      border: 1px solid black;
    }

    .card-black {
      background: black;
    }

    .card-soft {
      background:rgb(255, 228, 193);
    }

    .card-black p, .card-soft p {
      color: white !important;
    }

    .card-black h1, .card-soft h1 {
      color: white !important;
    }

    .report-content-container {
      display: inline-block;
    }

    .report-content-card p {
      font-size: 1.2rem;
      font-weight: bold;
    }

    .report-content-card h1 {
      font-size: 1rem;
      font-weight: 300;
    }

    th {
      background: rgb(250, 235, 215);
    }

    table {
      margin: 0 auto;
      min-width: 600px;
    }

    table,
    th,
    td {
      border: 1px solid black;
      border-collapse: collapse;
    }

    .centered-text {
      text-align: center;
    }

    .to-uppercase {
      text-transform: uppercase;
    }
    
    .p-3 {
      padding-left: 5px;
      padding-right: 5px;
    }

    @media print {
      .pg-break {
        clear: both;
        /* page-break-after: always; */
        page-break-before: always;
      }
    }
